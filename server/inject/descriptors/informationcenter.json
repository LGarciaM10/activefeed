{
  "type": "object",
  "comment": "issueslist",
  "properties": {
    "feedlist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectHidden": true
          },
          "name": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText"
          },
          "type": {
            "type": "string",
            "selector": "td",
            "selectIndex": 1,
            "control": "property.innerText"
          },
          "status": {
            "type": "string",
            "selector": "td",
            "selectIndex": 4,
            "control": {
              "fn": "function(param){return 'flex'}",
              "control": "property.innerText"
            }
          },
          "environment": {
            "type": "string",
            "selector": "td span",
            "selectIndex": 3,
            "control": "property.className"
          },
          "region": {
            "type": "string"
          },
          "site": {
            "type": "string"
          },
          "lastexecution": {
            "type": "date",
            "format": "MM/DD/YYYY HH:mm:ss"
          },
          "schedule": {
            "type": "string",
            "selector": "td",
            "selectIndex": 2,
            "control": "property.innerText"
          }
        }
      },
      "selectorAll": "#MyTable tbody tr[moduleid]"
    }
  },
  "connect": []
}
